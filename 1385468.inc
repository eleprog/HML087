	.db _1, _0, _1, _0, _1, _0, _0, _0
	.db _0, _0, _0, _0, _0, _0, _0, _0
	.db _0, _0, _0, _0, _0, _0, _0, _0
	.db _0, _0, _0, _0, _0, _1, _0, _1
	.db _0, _1, _1, _0, _1, _0, _1, _0
	.db _0, _1, _0, _1, _1, _0, _0, _0
	.db _1, _1, _0, _1, _1, _1, _1, _0